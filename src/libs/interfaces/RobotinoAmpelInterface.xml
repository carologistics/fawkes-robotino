<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE interface SYSTEM "interface.dtd">
<interface name="RobotinoAmpelInterface" author="Jens Wittmeyer" year="2012">
  <constants>
    <enum name="AmpelState">
      <comment>
	This determines the current status of skill execution.
      </comment>
      <item name="YELLOW"> true if ONLY yellow is active.</item>
      <item name="YELLOW_GREEN"> true if green and yellow is active.</item>
      <item name="GREEN">true if ONLY green is active.</item>
      <item name="RED">true if ONLY red is active.</item>
      <item name="YELLOW_FLASHING">true if yellow is falshing.</item>
    </enum>
  </constants>
  <data>
    <comment>
     Storage about the Ampel.
    </comment>
<field type="AmpelState" name="state">
</field>
  </data>
</interface>
