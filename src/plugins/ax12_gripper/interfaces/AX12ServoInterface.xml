<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE interface SYSTEM "interface.dtd">
<interface name="AX12ServoInterface" author="Tim Niemueller, Nicolas Limpert" year="2015">
  <data>
    <comment>
      Interface to access Robotis Dynamixel Servos
    </comment>
    <field type="uint32" name="model_number">Model number</field>
    <field type="string"  name="firmware_version">Firmware version</field>
    <field type="string"  name="baudrate">Baud rate</field>
    <field type="string"  name="delay_time">Return delay time</field>
    <field type="uint32" name="cw_angle_limit">Clockwise angle limit</field>
    <field type="uint32" name="ccw_angle_limit">Counter-clockwise angle limit</field>
    <field type="string"  name="temperature_limit">Temperature limit</field>
    <field type="string"  name="lowest_voltage_limit">Lowest voltage limit</field>
    <field type="string"  name="highest_voltage_limit">Highest voltage limit</field>
    <field type="uint32" name="max_torque">Max torque</field>
    <field type="string"  name="status_return_level">Status return level</field>
    <field type="string"  name="alarm_led">Alarm LED status</field>
    <field type="string"  name="alarm_shutdown">Shutdown on alarm state</field>
    <field type="uint32" name="down_calibration">Down calibration</field>
    <field type="uint32" name="up_calibration">Up calibration</field>
    <field type="bool"   name="torque_enabled">Torque enabled</field>
    <field type="bool"   name="led_enabled">LED enabled</field>
    <field type="string"  name="cw_margin">CW Compliance Margin</field>
    <field type="string"  name="ccw_margin">CCW Compliance Margin</field>
    <field type="string"  name="cw_slope">CW Compliance Slope</field>
    <field type="string"  name="ccw_slope">CCW Compliance Slope</field>
    <field type="uint32" name="goal_position">Goal position</field>
    <field type="uint32" name="goal_speed">Goal speed</field>
    <field type="float"  name="max_supported_speed">Max supported speed</field>
    <field type="uint32" name="torque_limit">Torque limit</field>
    <field type="uint32" name="position">Present position</field>
    <field type="uint32" name="speed">Present speed</field>
    <field type="uint32" name="load">Present load</field>
    <field type="string"  name="voltage">Present voltage</field>
    <field type="string"  name="temperature">Present temperature</field>
    <field type="bool"   name="moving">Moving</field>
    <field type="bool"   name="lock">Lock</field>
    <field type="uint32" name="punch">Punch</field>

    <field type="float"  name="left">Current left.</field>
    <field type="float"  name="right">Current right.</field>
    <field type="uint32" name="left_load">Current left load.</field>
    <field type="uint32" name="right_load">Current right load.</field>
    <field type="float"  name="angle">Current angle.</field>
    <field type="float"  name="offset">Current offset.</field>
  </data>
  <message name="set_id">
    <comment>Set ID</comment>
    <field type="string" name="id">New ID</field>
  </message>
 <message name="set_baudrate">
    <comment>Set baud rate</comment>
    <field type="string" name="baudrate">New baud rate</field>
  </message>
  <message name="set_return_delay_time">
    <comment>Set return delay time</comment>
    <field type="string" name="return_delay_time">New return delay time</field>
  </message>
  <message name="set_angle_limits">
    <comment>Set angle limits</comment>
    <field type="uint32" name="angle_limit_cw">New cw angle limit</field>
    <field type="uint32" name="angle_limit_ccw">New ccw angle limit</field>
  </message>
  <message name="set_temperature_limit">
    <comment>Set temperature limits</comment>
    <field type="string" name="temp_limit">New temp limit</field>
  </message>
  <message name="set_voltage_limits">
    <comment>Set voltage limits</comment>
    <field type="string" name="voltage_limit_low">New lower voltage limit</field>
    <field type="string" name="voltage_limit_high">New higher voltage limit</field>
  </message>
  <message name="set_max_torque">
    <comment>Set return delay time</comment>
    <field type="uint32" name="temp_limit">New temp limit</field>
  </message>
  <message name="set_status_return_level">
    <comment>Set status return level</comment>
    <field type="string" name="temp_limit">New status return level</field>
  </message>
  <message name="set_alarm_led">
    <comment>Set alarm led</comment>
    <field type="string" name="alarm_led">New alarm led value</field>
  </message>
  <message name="set_alarm_shutdown">
    <comment>Set shutdown on alarm</comment>
    <field type="string" name="alarm_shutdown">New shutdown on alarm value</field>
  </message>
  <message name="set_torque_enabled">
    <comment>Set torque enabled</comment>
    <field type="bool" name="alarm_shutdown">New torque enabled value</field>
  </message>
  <message name="set_led_enabled">
    <comment>Set LED enabled</comment>
    <field type="bool" name="led_enabled">New LED enabled value</field>
  </message>
  <message name="set_compliance_values">
    <comment>Set compliance values</comment>
    <field type="string" name="cw_margin">New CW compliance margin</field>
    <field type="string" name="ccw_margin">New CCW compliance margin</field>
    <field type="string" name="cw_slope">New CW compliance slope</field>
    <field type="string" name="ccw_slope">New LED enabled value</field>
  </message>
  <message name="set_goal_speed">
    <comment>Set goal speed</comment>
    <field type="uint32" name="goal_speed">New goal speed</field>
  </message>
  <message name="set_torque_limit">
    <comment>Set torque limit</comment>
    <field type="uint32" name="torque_limit">New torque limit</field>
  </message>
  <message name="set_punch">
    <comment>Set punch</comment>
    <field type="uint32" name="punch">New punch</field>
  </message>
  <message name="goto_position">
    <comment>Goto position</comment>
    <field type="uint32" name="position">New position</field>
  </message>

</interface>
