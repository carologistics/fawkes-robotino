(
	(defop step-drive-to
		:invocation( ! (step drive-to machine $mps-name side $side) )
		:context()  
		:body(
			;Sides allowed
			(? (MEMQ $side (. INPUT OUTPUT .)))
			(if (? (= $side INPUT) )
				(! (= $place (string-cat $mps-name "-I")))
				else 
				(! (= $place (string-cat $mps-name "-O")))
				)

			(! (execute (skill "drive_to" "place" $place) ))  
			)
		)
	)