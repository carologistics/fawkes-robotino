	(defop debug-achieveworkpiece
		:invocation (Achieve-WorkPiece $agent (workpiece $id $base $rings $cap $place) $status)
		:body ((! (printf (Format "Achieve-WorkPiece: %s %s %s %s %s %s %s \n" $agent $status $id $base $rings $cap $place))))
	  )

	)
